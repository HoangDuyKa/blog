<div class="mt-4">
    <h1>Create New Course</h1>
    <form method="POST" action="/courses/store" enctype="application/json">
        <div class="form-group">
            <label for="name">name</label>
            <input type="text" class="form-control" id="name" name="name">
        </div>
        <div class="form-group">
            <label for="descriptions">descriptions</label>
            <input type="text" class="form-control" id="descriptions" name="descriptions">
        </div>
        <div class="form-group">
            <label for="level">level</label>
            <input type="text" class="form-control" id="level" name="level">
        </div>
        <div class="form-group">
            <label for="videoId">videoId</label>
            <input type="text" class="form-control" id="videoId" name="videoId">
        </div>
    <button type="submit" class="btn btn-primary">Add Course</button>
</form>
</div>